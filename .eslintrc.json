{
    "env": {
        "node": true, //이거해줘야 process 에러 안남. 
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "rules": {
        "indent" : [
            "warn", 
            2
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-unused-vars" : 1,
        "eol-last": 2
    }
}